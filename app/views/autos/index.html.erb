<p id="notice"><%= notice %></p>

<h1>Listing Autos</h1>

<table>
  <thead>
    <tr>
      <th>Modelo</th>
      <th>Marca</th>
      <th>Precio</th>
      <th>Fecha venta</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @autos.each do |auto| %>
      <tr>
        <td><%= auto.modelo %></td>
        <td><%= auto.marca %></td>
        <td><%= auto.precio %></td>
        <td><%= auto.fecha_venta %></td>
        <td><%= link_to 'Show', auto, class: 'button radius'%></td>
        <td><%= link_to 'Edit', edit_auto_path(auto), class:'button radius success'%></td>
        <td><%= button_to "Destroy", auto, :method=>:delete, :class=>:destroy, data: { confirm: 'Are you sure?' } ,class:'button radius alert' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auto', new_auto_path, class: 'button round success' %>
